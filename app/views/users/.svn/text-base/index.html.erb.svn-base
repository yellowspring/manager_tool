<div class="span10">
  <br /><br /><br />
    
<h2 class="my_h2">Listing Users</h2>
    

  <table   class="stats" style="width: 100%">
    <thead>
      <tr>
        <th >Name</th>
        <th class="hed">Email</th>
        <th class="hed">Admin</th>
        <th class="hed">Comment</th>
        <th class="hed">Edit</th>
        <th class="hed">Delete</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td class="hed"><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.admin %></td>
          <td><%= user.comment %></td>
          <td><%= link_to raw('<i class="icon-edit"></i>'), edit_user_path(user) %></td>
          <td><%= link_to raw('<i class="icon-trash "></i>'), user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

 
 
<script>
 
$('#tb tbody tr:odd').css("backgroundColor","#fcf8e3");
$('#tb tbody tr:even').css("backgroundColor","#d9edf7");
</script>

 
<script>
$(function(){
    $('#comments').editable({
        url: '/post',
        title: 'Enter comments'
    });
});
</script>

<br />



<!--! %= debug(params) %>-->

 
</div>
