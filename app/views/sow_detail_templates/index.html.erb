    <td><%= link_to 'Show', sow_detail_template %></td>
    <td><%= link_to 'Edit', edit_sow_detail_template_path(sow_detail_template) %></td>
    <td><%= link_to 'Destroy', sow_detail_template, confirm: 'Are you sure?', method: :delete %></td>
